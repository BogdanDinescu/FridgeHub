{
    "device-name": "FridgeHub Android",
    "device-type": "Fridge",
    "buffers-count": 6,
    "input-buffers": {
        "1":{
            "token-delimitators": "",
            "protocol": "HTTP",
            "prefix": "http://localhost:8080/",
            "buffer-tokens":[
            {
                "name": "settings/temp/",
                "description": "Seteaza temperatura frigiderului",
                "token-type": "number",
                "byte-size": 4,
                "regex-rule": "[+-]?([0-9]*[.])?[0-9]+",
                "optional": "false"
                
            }
            ]
        },
        "2": {
            "token-delimitators": "",
            "protocol": "HTTP",
            "prefix": "http://localhost:8080/",
            "buffer-tokens":[
            {
                "name": "items/remove/",
                "description": "Sterge un produs",
                "token-type": "string",
                "byte-size": 256,
                "regex-rule": "[a-zA-Z]",
                "optional": "true"
                
            }
            ]
        },
        "3": {
            "token-delimitators": "",
            "protocol": "HTTP",
            "prefix": "http://localhost:8080/",
            "buffer-tokens":[
            {
                "name": "items/add/",
                "description": "Adauga un produs in frigider",
                "token-type": "json",
                "byte-size": 256,
                "regex-rule": "[ \t\n]*?{[ \t\n]*?\"name\" ?: ?\"[a-zA-Z]*\",[ \t\n]*?\"itemExpDate\" ?: ?\"[0-9]*\/[0-9]*\/[0-9]*\",[ \t\n]*?\"weight\" ?: ? ([0-9]*[.])?[0-9]+,[ \t\n]*?\"calories\" ?: ?[0-9]*[ \t\n]*}",
                "optional": "false"
                
            },
            {
                "name": "update",
                "description": "Trimite un json cu un produs si il modifica daca exista",
                "token-type": "json",
                "byte-size": 256,
                "regex-rule": "[ \t\n]*?{[ \t\n]*?\"name\" ?: ?\"[a-zA-Z]*\",[ \t\n]*?\"itemExpDate\" ?: ?\"[0-9]*\/[0-9]*\/[0-9]*\",[ \t\n]*?\"weight\" ?: ? ([0-9]*[.])?[0-9]+,[ \t\n]*?\"calories\" ?: ?[0-9]*[ \t\n]*}",
                "optional": "false"
                
            },
            {
                "name": "calories",
                "description": "calculeaza caloriile pentru o anumita cantitate a unor produse date in body ca json",
                "token-type": "json",
                "byte-size": 256,
                "regex-rule": "\[[ \t\n]*?{[ \t\n]*?\"name\" ?: ?\"[a-zA-Z]*\",[ \t\n]*?\"weight\" ?: ? ([0-9]*[.])?[0-9]+[ \t\n]*?}[ \t\n]*?,?[ \t\n]*?\]",
                "optional": "false"
                
            },
            ]
        },
    },
    "output-buffers":{
        "1":{
            "token-delimitators": " ",
            "protocol": "HTTP",
            "prefix": "http://localhost:8080/",
            "caller":"remote",
            "buffer-tokens":[
            {
                "name": "temp",
                "description": "Obtine temperatura frigiderului",
                "token-type": "string",
                "byte-size": 4,
                "regex-rule": "[+-]?([0-9]*[.])?[0-9]+",
                "optional": "false"
                
            }
            ]
        },
        "2": {
            "token-delimitators": " ",
            "protocol": "HTTP",
            "prefix": "http://localhost:8080/",
            "caller":"remote",
            "buffer-tokens":[
            {
                "name": "items/",
                "description": "Obtine un produs din frigider",
                "token-type": "string",
                "byte-size": 256,
                "regex-rule": "[a-zA-Z]*",
                "optional": "true"
                
            },
            {
                "name": "items/expired",
                "description": "Obtine numele produselor care au expirat",
                "token-type": "string",
                "byte-size": 256,
                "regex-rule": "[[a-zA-Z]*[ ]*?,[ ]?]*[a-zA-Z]*",
                "optional": "false"
                
            },
            {
                "name": "items/getAll",
                "description": "Intoarce toate numele produselor din frigider",
                "token-type": "string",
                "byte-size": 256,
                "regex-rule": "[[a-zA-Z]*[ ]*?,[ ]?]*[a-zA-Z]*",
                "optional": "false"
                
            }
            ]
        },
        "3": {
            "token-delimitators": " ",
            "protocol": "HTTP",
            "prefix": "http://localhost:8080/",
            "caller":"remote",
            "buffer-tokens":[
            {
                "name": "totalCalories",
                "description": "calculeaza numarul de calorii al cantitatii de produse care nu au expirat",
                "token-type": "number",
                "byte-size": 4,
                "regex-rule": "[0-9]*",
                "optional": "false"
                
            },
            ]
        }
    }
}




